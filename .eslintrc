{
  "env": {
    "browser": true,
    "es6": true
  },

  "plugins": [
    "react"
  ],

  "globals": {
    "process": true
  },

  "parserOptions": {
    "sourceType": "module",
  },

  "rules": {
    "no-unused-vars": ["error", { "vars": "all", "args": "none" }]
  },

  "extends": ["eslint:recommended", "plugin:react/recommended"],
}
